const express = require('express');
const { getTasks, createTask, updateTask, deleteTask } = require('../controllers/taskControllerORM');

const router = express.Router();

router.get('/angestellteORM', getTasks);
router.post('/angestellteORM', createTask);
router.put('/angestellteORM/:id', updateTask);
router.delete('/angestellteORM/:id', deleteTask);

module.exports = router;
